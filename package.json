{
  "name": "student-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean": "react-native start --reset-cache",
    "lint": "eslint .",
    "test": "jest",
    "doctor": "react-native doctor",
    "android-clean": "cd android && gradlew clean && cd ..",
    "fresh-install": "npm run android-clean && npx react-native run-android --reset-cache",
    "device-check": "adb devices",
    "restart-adb": "adb kill-server && adb start-server",
    "logs": "adb logcat | findstr ReactNativeJS",
    "debug-logs": "adb logcat *:E",
    "fix-permissions": "adb shell pm grant com.reactnative android.permission.SYSTEM_ALERT_WINDOW"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "invariant": "^2.2.4",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-fs": "^2.20.0",
    "react-native-pdf-lib": "^1.0.0",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-share": "^12.2.4",
    "react-native-svg": "^15.15.1",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "12.3.6",
    "@react-native-community/cli-platform-android": "12.3.6",
    "@react-native-community/cli-platform-ios": "12.3.6",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
